			END IF
			' Recuperer les Parametres
			Param = MID(Commande, CommPosition + TailleComm + 2)
			
			SCOPE
			' ====================================================================
			
			Dim RapidAdd				as Boolean = false
			Dim PosPP					as Integer = 0
			Dim PosMM					as Integer = 0
